Ui4.CornerRadius = UDim.new(0,4)
Ui4.Parent = ButtonScript3
FrameScript3.Active = false
FrameScript3.BackgroundColor3 = Color3.fromRGB(33,33,33)
FrameScript3.Selectable = false
FrameScript3.Size = UDim2.new(0, 463,0, 34)
FrameScript3.AutoLocalize = true
FrameScript3.Parent = Scroll

TextScript3.BackgroundTransparency = 1
TextScript3.Position = UDim2.new(0,0,0,0)
TextScript3.Size = UDim2.new(0, 120,0, 34)
TextScript3.Font = Enum.Font.SourceSansBold
TextScript3.Text = "AUTO KILL:OFF"
TextScript3.TextSize = 19
TextScript3.TextStrokeTransparency = 1
TextScript3.TextColor3 = Color3.fromRGB(255, 255, 255)
TextScript3.Parent = FrameScript3
TextScript3.AutoLocalize = true

ButtonScript3.AutoButtonColor = true
ButtonScript3.BackgroundColor3 = Color3.fromRGB(44,44,44)
ButtonScript3.Position = UDim2.new(0.82, 0,0.118, 0)
ButtonScript3.Size = UDim2.new(0, 76, 0, 26)
ButtonScript3.AutoLocalize = true
ButtonScript3.Font = Enum.Font.SourceSansBold
ButtonScript3.Text = "EXECUTE"
ButtonScript3.TextSize = 16
ButtonScript3.TextColor3 = Color3.fromRGB(255, 255, 255)
ButtonScript3.Parent = FrameScript3
ButtonScript3.MouseButton1Down:connect(function()
	if AutoKilling == false then
		SoundClick2:Play()
		TextScript3.Text = "AUTO KILL:ON"
		AutoKilling = true
		AutoClick = true
	elseif AutoKilling == true then
		SoundClick2:Play()
		TextScript3.Text = "AUTO KILL:OFF"
		AutoKilling = false
		AutoClick = false
	end
end)
