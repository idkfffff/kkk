Close.MouseButton1Down:connect(function()
	Main.Visible = false
	Open.Visible = true
end)

EspName.Name = "Earned All Chest"
EspName.Parent = Main
EspName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
EspName.BackgroundTransparency = 0.900
